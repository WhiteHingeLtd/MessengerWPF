<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             x:Class="MessengerWPF.UserControls.SelfMessage"
    xmlns:Ui="clr-namespace:EmojiBox.Ui;assembly=EmojiBox"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="300">
    <Grid HorizontalAlignment="Right">
        <Border BorderThickness="1" HorizontalAlignment="Right" VerticalAlignment="Top" Background="#FF422FFF" CornerRadius="8">
            <Ui:EmojiRichTextBox x:Name="FromMessageBox" Margin="5" HorizontalAlignment="Right" VerticalAlignment="Top" Foreground="White" BorderBrush="{x:Null}" Background="{x:Null}" IsReadOnly="True"/>
        </Border>
    </Grid>
</UserControl>
